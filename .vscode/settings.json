{
  "[powershell]": {
    "editor.semanticHighlighting.enabled": true,
    "editor.tabSize": 4
  },
  "editor.insertSpaces": true,
  "editor.rulers": [
    120
  ],
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.branchProtection": [
    "main"
  ],
  "markdownlint.config": {
    "MD013": false,
    "MD024": false,
    "MD029": false,
    "MD033": {
      "allowed_elements": [
        "kbd"
      ]
    },
    "MD034": false,
    "MD038": false
  }
}
